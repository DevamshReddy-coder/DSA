import java.util.*;
class Solution {
    public String reverse(String S) {
        // code here
        Stack<Character> st=new Stack<>();
        int idx=0;
        while(idx<S.length()){
            st.push(S.charAt(idx));
            idx++;
        }
        StringBuilder res=new StringBuilder("");
        while(!st.isEmpty()){
            char curr=st.pop();
            res.append(curr);
        }
        return res.toString();
    }
}
